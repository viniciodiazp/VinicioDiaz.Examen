<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="VinicioDiaz.Examen.Login">
    <ContentPage.Content>
        <StackLayout>
            <Image Source="https://sbsconsulting.com.ec/wp-content/uploads/2018/01/LOGO-UISRAEL-768x209.png " />
            <Entry x:Name="txtUsuario" Text="" Placeholder="Usuario"></Entry>
            <Entry x:Name="txtContrasena" IsPassword="true" Text="" Placeholder="Contraseña"></Entry>
            <Button x:Name="btnLogin" Text="Iniciar sesión" Clicked="btnLogin_Clicked"></Button>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>